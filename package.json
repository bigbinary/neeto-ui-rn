{
  "name": "@bigbinary/neetoui-rn",
  "version": "0.0.6",
  "main": "lib/index.js",
  "author": "BigBinary",
  "license": "MIT",
  "scripts": {
    "android": "yarn run prestorybook && react-native run-android",
    "ios": "yarn run prestorybook && react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "precommit": "lint-staged",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "build-react": "webpack --mode production",
    "start-react": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build-storybook-web": "./node_modules/@storybook/react/bin/build.js",
    "storybook-web": "yarn run prestorybook && ./node_modules/@storybook/react/bin/index.js"
  },
  "files": [
    "lib"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,es6,jsx,ts,tsx}": [
      "./node_modules/prettier/bin-prettier.js --write",
      "prettier --write",
      "eslint --fix"
    ]
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-async-storage/async-storage": "^1.15.11",
    "@react-native-community/eslint-config": "^3.0.1",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/cli": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/react-native": "^5.3.25",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^3.11.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.66.2",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-web": "^0.17.5",
    "react-test-renderer": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bigbinary/neeto-ui-rn.git"
  },
  "keywords": [
    "components",
    "ui",
    "styles",
    "layouts",
    "react-native"
  ],
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "storybook/stories"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
