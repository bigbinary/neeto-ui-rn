{
  "name": "@bigbinary/neetoui-rn",
  "version": "0.0.140",
  "main": "lib/index.js",
  "author": "BigBinary",
  "license": "MIT",
  "description": "The neetoUI-RN library drives the experience in all neeto product's React Native applications built at BigBinary.",
  "scripts": {
    "android": "yarn prestart && react-native run-android",
    "ios": "yarn prestart && react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "build": "babel src --out-dir lib",
    "lint": "eslint .",
    "format": "prettier --write .",
    "flow": "flow",
    "prestart": "sb-rn-get-stories",
    "storybook-watcher": "sb-rn-watcher",
    "storybook": "start-storybook -p 7007",
    "build-react": "webpack --mode production --config webpack.config.js",
    "serve": "serve -s dist",
    "start-react": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build-storybook-web": "./node_modules/@storybook/react/bin/build.js",
    "storybook-web": "yarn prestart && ./node_modules/@storybook/react/bin/index.js",
    "get-stories": "sb-rn-get-stories --config-path=./.storybook",
    "preweb": "yarn get-stories",
    "web": "yarn prestart && webpack serve --mode=development --config webpack.config.js",
    "prepare": "husky install"
  },
  "files": [
    "lib",
    "src",
    "react-native.config.js",
    "assets"
  ],
  "lint-staged": {
    "src/**/*.{js,es6,jsx,ts,tsx}|storybook/stories**/*.js|.storybook/preview.js|.storybook/main.js|.storybook/Storybook.js": [
      "./node_modules/prettier/bin-prettier.js --write",
      "prettier --write",
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "react-native-safe-area-context": "^3.3.2",
    "react-native-svg": "*",
    "react-native-webview": "11.14.4"
  },
  "dependencies": {
    "@react-native-community/hooks": "^2.8.1",
    "@styled-system/prop-types": "^5.1.5",
    "dayjs": "^1.11.4",
    "react-native-calendars": "^1.1285.0",
    "react-native-gesture-handler": "^2.4.0",
    "react-native-material-ripple": "^0.9.1",
    "react-native-modal": "^13.0.0",
    "react-native-pell-rich-editor": "^1.8.6",
    "react-native-popover-view": "^4.1.0",
    "react-native-reanimated": "^2.7.0",
    "react-native-remix-icon": "^0.3.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-toast-message": "^2.1.0",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-async-storage/async-storage": "^1.15.11",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-native-community/slider": "^4.1.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-controls": "^6.3.12",
    "@storybook/addon-essentials": "^6.4.0-beta.33",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^6.0.1-alpha.6",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-alpha.6",
    "@storybook/addon-ondevice-controls": "^6.0.1-alpha.6",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-ondevice-notes": "^6.0.1-alpha.6",
    "@storybook/addon-storyshots": "^6.3.12",
    "@storybook/addons": "^6.3.1",
    "@storybook/cli": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/react-native": "^6.0.1-alpha.6",
    "@storybook/react-native-server": "^6.0.0-alpha.0",
    "@svgr/webpack": "^5.5.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-native-web": "^0.17.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-neeto": "^1.0.10",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^3.11.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.0.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.3.1",
    "ramda": "^0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.66.2",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-svg": "^12.1.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-web": "^0.17.5",
    "react-native-webview": "11.14.4",
    "react-test-renderer": "17.0.2",
    "serve": "^13.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native)/"
    ],
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bigbinary/neeto-ui-rn.git"
  },
  "keywords": [
    "components",
    "ui",
    "styles",
    "layouts",
    "react-native"
  ],
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "storybook/stories"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
