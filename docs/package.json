{
  "name": "@neeto-ui-rn/docs",
  "version": "0.0.1",
  "scripts": {
    "android": "yarn run prestorybook && react-native run-android",
    "ios": "yarn run prestorybook && react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "build-react": "webpack --mode production",
    "start-react": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build-storybook-web": ".././node_modules/@storybook/react/bin/build.js",
    "storybook-web": ".././node_modules/@storybook/react/bin/index.js"
  },
  "dependencies": {
    "@neeto-ui-rn/ui": "0.1.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-web": "^0.14.10",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^6.1.12",
    "@storybook/addon-essentials": "^6.1.12",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^6.1.12",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.23",
    "@storybook/react": "^6.1.12",
    "@storybook/react-native": "^5.3.23",
    "@storybook/react-native-server": "^5.3.23",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^6.5.1",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-dom": "^17.0.1",
    "react-native-storybook-loader": "^2.0.2",
    "react-test-renderer": "16.13.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "storybook/stories"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
